  "description": "Fees and fines for bursar export parameters",
  "BursarFeeFines": {
    "type": "object",
    "properties": {
      "ftpUrl": {
        "type": "string",
        "description": "Url to ftp server"
      },
      "daysOutstanding": {
        "type": "integer",
        "description": "Days outstanding"
      },
      "patronGroups": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    }
  }
}
